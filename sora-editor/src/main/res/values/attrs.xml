<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CodeEditor">
        <attr name="editorCornerRadius" format="dimension" />
        <attr name="editorColorScheme">
            <enum name="normal" value="0" />
            <enum name="darcula" value="1" />
            <enum name="eclipse" value="2" />
            <enum name="github" value="3" />
            <enum name="notepadXX" value="4" />
            <enum name="vs2019" value="5" />
        </attr>
        <attr name="editorText" format="string" />
        <attr name="editorTextMateEnabled" format="boolean" />
    </declare-styleable>
</resources>